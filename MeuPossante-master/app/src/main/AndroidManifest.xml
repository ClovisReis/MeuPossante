<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lucas.com.meupossante">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Agua"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".Pneus"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity
            android:name=".Manutencao"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ManutencaoItem"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Cadastro"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Configuracoes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Rastreio"
            android:theme="@style/AppTheme.NoActionBar" />

        <service
            android:name=".RastreioService"
            android:exported="false" />

        <activity
            android:name=".ListaOficinas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AlertaComponentes"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Termos"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Splash"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Quilometragem"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>